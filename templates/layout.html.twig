			                       <button type="button" class="btn btn btn-dark" data-dismiss="modal">Fermer</button>
			                  </div>
			              </div>
			          </div>
			             
			      </div>
			    </div>
			  </div>
			</div>

			<!-- END PupUp -->

			{% if inscriptionReussite is defined %}
			<!-- inscription success Model -->

			<div class="modal fade show" id="successModal" style="display: block; padding-right: 14px;" tabindex="-1" role="dialog" aria-labelledby="identificationModalLabel" aria-hidden="true">
			  <div class="modal-dialog" role="document">
			    <div class="modal-content">
			      <div class="modal-header default-bg border-bottom border-warning text-center">
		    		<div class="container-fluid">
		    			<div class="row">
			    			<div class="col-md-2"></div>
			    			<div class="col-md-8"> <h5 class="modal-title text-center ml-3" id="identificationModalLabel">Dernière étape</h5></div>
			    			<div class="col-md-2"></div>
		    			</div>
		    		</div>
			        <a href="home/" class="close"><span aria-hidden="true">&times;</span></a>
			      </div>
			      <div class="modal-body">
			      	  <div class="container-fluid">
						<div class="row">
			    			<div class="alert alert-success" role="alert">
							  <h4 class="alert-heading">Félicitations ! Votre nouveau compte a été créé avec succès !</h4><br>
