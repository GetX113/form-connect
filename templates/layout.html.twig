			{% endif %}
			<!-- Connexion Organisme Model -->

			<div class="modal fade {% if echecConnexionOrganisme is defined %}show{% endif %}{% if echecActivationEmailOrganisme is defined %}show{% endif %}" {% if echecConnexionOrganisme is defined %}style="display: block; padding-right: 14px;"{% endif %}{% if echecActivationEmailOrganisme is defined %}style="display: block; padding-right: 14px;"{% endif %} id="connexionOrganismeModal" tabindex="-1" role="dialog" aria-labelledby="connexionOrganismeModalLabel" aria-hidden="true">
