			      	  <div class="container-fluid">
						<div class="row">
			    			<div class="col-md-2"></div>
			    			<div class="col-md-8"> <h3 class="text-center">Vous êtes ?</h3></div>
			    			<div class="col-md-2"></div>
						</div>
					  </div>
			      	  <div class="container-fluid mt-5">
			              <div class="row">
			              	    <div class="col-md-2"></div>
			                    <div class="col-md-4">
			                  	    <button data-toggle="modal" type="email" data-target="#inscriptionOrganismeModal" class="btn-circle btn-circle-xl m-1" data-dismiss="modal"> <img src="/scs.png"> </button><br>
			                  	    <div class="container-fluid ml-4">
				                  	  	<div class="row">
			                  	         	<h5 class="text-primary">Organisme</h5>
			                  	        </div>
			                  	    </div>     
			                    </div>
			                    <div class="col-md-4">
			                  	    <button data-toggle="modal" data-target="#inscriptionFormateurModal" class="btn-circle btn-circle-xl m-1" data-dismiss="modal"> <img src="/frmts.png"> </button><br>
			                  	    <div class="container-fluid ml-4">
			                  	    	<div class="row text-md-center">
			                  	    		<h5 class="text-primary">Formateur</h5>
				                  	    </div>
			                  	    </div>
			                  	    
			                    </div>
			                  <div class="col-md-2"></div>
			              </div>
			          </div>
